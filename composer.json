{
    "name": "xola/omnipay-bundle",
    "type": "symfony-bundle",
    "description": "Integrates Omnipay with Symfony2+",
    "keywords": ["omnipay", "payment", "credit card", "gateway", "symfony"],
    "homepage": "https://github.com/xola/OmnipayBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1 <4.0",
        "omnipay/common": "~2.0",
        "guzzle/http": "~3.7",
        "guzzle/plugin": "~3.7",
        "guzzle/log": "~3.7",
        "psr/log": "1.0"
    },
    "require-dev": {
        "omnipay/omnipay": "~2.0",
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-0": {
            "Xola\\OmnipayBundle": ""
        }
    },
    "target-dir": "Xola/OmnipayBundle"
}
