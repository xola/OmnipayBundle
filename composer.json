{
    "name": "xola/omnipay-bundle",
    "type": "symfony-bundle",
    "description": "Integrates Omnipay 3 with Symfony 2+",
    "keywords": [
        "omnipay",
        "payment",
        "credit card",
        "gateway",
        "symfony"
    ],
    "homepage": "https://github.com/xola/OmnipayBundle",
    "license": "MIT",
    "require": {
        "php": ">=7",
        "symfony/framework-bundle": ">=2.1 <4.0",
        "league/omnipay": "^3.0",
        "php-http/logger-plugin": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "omnipay/authorizenet": "^3.3",
        "omnipay/eway": "^3.0",
        "omnipay/mollie": "^5.2",
        "omnipay/paymentexpress": "^3.0",
        "omnipay/paypal": "^3.0",
        "omnipay/sagepay": "^3.2",
        "omnipay/securepay": "^3.0",
        "omnipay/stripe": "^3.1",
        "omnipay/worldpay": "^3.0",
        "symfony/yaml": "^3.4"
    },
    "autoload": {
        "psr-0": {
            "Xola\\OmnipayBundle": ""
        }
    },
    "target-dir": "Xola/OmnipayBundle"
}
